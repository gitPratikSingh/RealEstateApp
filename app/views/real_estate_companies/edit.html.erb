<h1>Edit Real State Company</h1>

<%= form_for @real_estate_company, url: realEstateCompany_path do |f| %>
  <p>
    <%= f.label :name  %><br>
    <%= f.text_field :name  %><br>
  </p>

  <p>
    <%= f.label :website  %><br>
    <%= f.text_field :website  %><br>
  </p>

  <p>
    <%= f.label :address  %><br>
    <%= f.text_field :address  %><br>
  </p>

  <p>
    <%= f.label :size  %><br>
    <%= f.text_field :size  %><br>
  </p>

  <p>
  <p>
    <%= f.submit %>
  </p>

<% end %>

<%= link_to 'Back', realEstateCompany_path %>