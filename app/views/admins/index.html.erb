<h1> List of Admins</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Name</th>
    <th>Password</th>
  </tr>

  <% @admins.each do |admin| %>
    <tr>
      <td><%= admin.email %></td>
      <td><%= admin.name %></td>
      <td><%= link_to 'Show', admin_path(admin) %></td>
      <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
    </tr>
  <% end %>
</table>